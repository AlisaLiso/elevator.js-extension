{
  "manifest_version": 2,
  "name": "ElevatorExtension",
  "description": "'Back to top' button that behaves like a real elevator",
  "version": "1.0.0",
  "icons": {
    "16": "imgs/favicon-16.png",
    "32": "imgs/favicon-32.png",
    "48": "imgs/favicon-48.png",
    "128": "imgs/favicon-128.png"
  },
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": [
        "bundle.js"
      ],
      "css": [
        "css/index.css"
      ]
    }
  ],
  "web_accessible_resources": [
    "media/*.*"
  ]
}
